# PostgreSQL Secrets
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGE_ME
POSTGRES_DB=n8n

# n8n Database Password (must match POSTGRES_PASSWORD)
DB_POSTGRESDB_PASSWORD=CHANGE_ME

# n8n Encryption Key (CRITICAL - do not lose)
N8N_ENCRYPTION_KEY=CHANGE_ME

# PostgreSQL Connection String (for monitoring)
# Must match POSTGRES_PASSWORD above
POSTGRES_CONNECTION=postgresql://n8n:CHANGE_ME@postgres:5432/n8n?sslmode=disable
